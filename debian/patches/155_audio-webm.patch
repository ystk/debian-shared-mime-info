From: =?UTF-8?q?Sebastian=20Dr=C3=B6ge?= <sebastian.droege@collabora.co.uk>
Date: Sat, 29 May 2010 14:45:28 +0200
Subject: [PATCH 2/3] Add audio/webm mimetype

---
 freedesktop.org.xml.in |    5 +++++
 1 files changed, 5 insertions(+), 0 deletions(-)

diff --git a/freedesktop.org.xml.in b/freedesktop.org.xml.in
index b2244a7..0d4a3f3 100644
--- a/freedesktop.org.xml.in
+++ b/freedesktop.org.xml.in
@@ -2218,6 +2218,11 @@ command to generate the output files.
       <match type="string" value="webm" offset="8"/>
     </magic>
   </mime-type>
+  <mime-type type="audio/webm">
+    <_comment>WebM audio</_comment>
+    <generic-icon name="audio-x-generic"/>
+    <sub-class-of type="video/webm"/>
+  </mime-type>
   <mime-type type="application/mxf">
     <_comment>MXF video</_comment>
     <acronym>MXF</acronym>
